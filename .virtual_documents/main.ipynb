        tmp_inner = tmp.copy()
        tmp_inner.loc[tmp_inner.index[ix], 'stated_preference_cont'] = long_df['stated_preference_cont'].max()
        counter_choice_probs.append(choice_probs(d=tmp_inner, mod=mod).iloc[ix])
mods.append(mod)
